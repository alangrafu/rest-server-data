SELECT ?date ?source ?series ?group ?hash ?datasetId ?datasetDate ?datasetOriginalUrl ?specificType ?parentViz WHERE {
    GRAPH <{{uri}}>{
                <{{uri}}> dcterms:created ?date;
                          dcterms:source ?source ;
                          a ?specificType;
                          vizon:hasParameter [ rdfs:label "series"; vizon:parameterValue ?series ];
                          vizon:hasParameter [ rdfs:label "group"; vizon:parameterValue ?group ]
     OPTIONAL{ <{{uri}}> prov:wasDerivedFrom ?parentViz }
    }
    GRAPH <{{lodspk.namedgraph}}>{
      ?datasetId void:dataDump ?source ;
                 nfo:hasHash [ nfo:hashValue ?hash ] ;
                 dcterms:created ?datasetDate ;
                 dcterms:source ?datasetOriginalUrl .
    }
}
