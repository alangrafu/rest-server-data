SELECT ?date ?source ?series ?group  WHERE {
    GRAPH <{{uri}}>{
                <{{uri}}> dcterms:created ?date;
                          dcterms:source ?source ;
                          vizon:hasParameter [ rdfs:label "series"; vizon:parameterValue ?series ];
                          vizon:hasParameter [ rdfs:label "group"; vizon:parameterValue ?group ]
    }
}
